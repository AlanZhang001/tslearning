{
    "compilerOptions": {
        "module": "commonjs",
        "allowSyntheticDefaultImports":true,
        "moduleResolution": "node",
        "allowJs": true,
        "target": "es5",
        "outDir": "app-build",
        "baseUrl": ".",
        "listEmittedFiles": true
    },
    "include": [
        "./app/**/*"
    ],

    "noEmitOnError": true
}
